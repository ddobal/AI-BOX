source "https://rubygems.org"

# 기본 Jekyll
gem "jekyll", "~> 4.3"
gem "webrick"

# Jekyll 플러그인들 (지금 _config.yml 에서 켜둔 것만)
group :jekyll_plugins do
  gem "jekyll-feed"
  gem "jekyll-seo-tag"
  gem "jekyll-sitemap"
end

# al-folio 테마를 Gem으로 사용
gem "al-folio", git: "https://github.com/alshedivat/al-folio.git"
